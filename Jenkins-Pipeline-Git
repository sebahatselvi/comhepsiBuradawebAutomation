pipeline{
    agent any
    
     environment{
    PATH= "/usr/local/Cellar/maven/3.6.2/libexec/bin:$PATH"}
    
    stages{
        stage("Github checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/sebahatselvi/comhepsiBuradawebAutomation'
            }
             stage("Maven Build"){
                 steps{
                   sh "mvn clean package"
            }
        }
    }
}
